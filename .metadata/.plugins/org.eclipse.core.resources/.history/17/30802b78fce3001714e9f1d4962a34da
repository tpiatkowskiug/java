
public class Pracownik {
String imie;
String nazwisko;
double pensja=0;

String getimie(){
	return imie;
}
void setimie(String imie){
	this.imie = imie;
}
String getnazwisko(){
	return nazwisko;
}
void setnazwisko(String nazwisko){
	this.nazwisko=nazwisko;
}
double getpensja(){
	return pensja;
}
void setpencja(double pensja){
	this.pensja=pensja;
}
void setPracownik(String imie, String nazwisko, double pensja){
	this.imie=imie;
	this.nazwisko=nazwisko;
	this.pensja=pensja;
}
String getPracownik(){
	return imie + nazwisko + (double)pensja;
}
}
