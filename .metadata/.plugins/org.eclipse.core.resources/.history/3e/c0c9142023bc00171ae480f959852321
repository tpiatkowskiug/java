import java.io.FileNotFoundException;
import java.io.IOException;
import java.io.RandomAccessFile;

public class Strumieniedwa {

	public static void main(String[] args) {
		RandomAccessFile stumienie = null;
		int iloscbajtow = 0;
		try{
			strumienie = new RandomAccessFile("dane.txt" ,"rw");
		} catch(FileNotFoundExceptnion e){
			System.out.println("Błąd otwierania strumienia");
		}
		try{
			strumienie.writeUTF("Może w końcu coś wyjdzie");
			strumienie.writeINT(20);
		}
		

	}

}
