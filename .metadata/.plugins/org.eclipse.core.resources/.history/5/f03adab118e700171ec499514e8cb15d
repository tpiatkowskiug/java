package okiennko;

import javax.swing.JFrame;

public class MyFrame extends JFrame{
	
		 
	    public MyFrame() {
	        super("Hello World");
	        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	        setVisible(true);
	        setLocation(300,100); 
	        //setResizable(false); 
	        setSize(400,400); 
	    }
	
}
