import java.io.DataOutputStream;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import java.io.File;
import java.util.Scanner;
import java.io.PrintWriter;
 
public class Strumienie {

	public static void main(String[] args) throws FileNotFoundException {
		Scanner podajimie = new Scanner(System.in);
		String imie = podajimie.nextLine();
		
		PrintWriter zapis = new PrintWriter("imie.txt");
		zapis.print(imie);
		zapis.close();
		
		File odczyt = new File("imie.txt");
		Scanner wyswietl = new Scanner(odczyt);
		System.out.println(wyswietl);
		
	}
	

}
